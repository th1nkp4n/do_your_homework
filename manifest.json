{
  "manifest_version": 2,
  "name": "do_your_homework extension",
  "version": "1.0",

  // Recommended
  "default_locale": "en",
  "description": "Hopefully we help you be more productive",
  "icons": {"48": "duh-icon.png"},

  "browser_action": {
    "default_popup": "status.html",
    "default_name":"Check Status"
  },
  "browser_action": {
    "default_popup": "alert.html",
    "default_name":"Alert"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
